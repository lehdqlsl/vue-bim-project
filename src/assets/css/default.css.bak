/* 회원가입 */
.join_step{margin-bottom:50px;}
.join_step ol{margin:auto;width:520px;}
.join_step ol::after{display:block;content:'';clear:both;}
.join_step ol li{position:relative;float:left;margin-right:110px;padding-top:60px;width:100px;height:100px;font-size:14px;font-weight:bold;text-align:center;color:#666;background-color:#e6e6e6;background-repeat:no-repeat;border-radius:100%;box-sizing:border-box;}
.join_step ol li.ok{color:#fff;background-color:#39bdff;}
.join_step ol li:first-child{background-image:url("/images/injeinc/icon_joinstep_01_off.png");background-position:center 25px;}
.join_step ol li:first-child.ok{background-image:url("/images/injeinc/icon_joinstep_01_on.png");}
.join_step ol li:nth-child(2){background-image:url("/images/injeinc/icon_joinstep_02_off.png");background-position:center 21px;}
.join_step ol li:nth-child(2).ok{background-image:url("/images/injeinc/icon_joinstep_02_on.png");}
.join_step ol li:last-child{margin-right:0;background-image:url("/images/injeinc/icon_joinstep_03_off.png");background-position:center 24px;}
.join_step ol li:last-child.ok{background-image:url("/images/injeinc/icon_joinstep_03_on.png");}
.join_step ol li::after{position:absolute;display:block;content:'';top:50%;right:-110px;width:110px;height:1px;background:#e6e6e6;}
.join_step ol li:last-child:after{display:none;}

