#skipNav {font-size: 0;height: 0;left: 0;line-height: 0;overflow: hidden;position: absolute;top: 0;visibility: hidden;width: 0;}
hr{margin:40px 0;border:none;}
.divGroup{margin:0 -10px;}
.divGroup:after{display:block;content:'';clear:both;}
.divGroup > div{float:left;padding:0 10px;box-sizing:border-box;}
.divGroup.cols2 > div{width:50%;}
.divGroup.cols3 > div{width:33.3333%;}
.divGroup.cols4 > div{width:25%;}
.divGroup.cols5 > div{width:20%;}

input[type='text'],input[type='password'],select{height:30px;max-width:100%;border:1px #c5cad4 solid;box-sizing:border-box;}
select{padding:0 20px 0 5px;background:#fff url(/images/injeinc/bg_selectBox.png) no-repeat right center;-webkit-appearance:none;}
textarea{padding:5px;border:1px #c5cad4 solid;box-sizing:border-box;resize:vertical;}

.tableBox{margin-bottom:30px;border-top:1px #0f5ba9 solid;overflow:hidden;}
.tableBox table{width:100%;border:0px none;}
.tableBox table th,
.tableBox table td{border-bottom:1px #d9dce2 solid;box-sizing:border-box;}
.tableBox table th{background:#f0f2f4;font-weight:bold;color:#333;}

.tableBox table.list{text-align:center;}
.tableBox table.list th,
.tableBox table.list td{padding:14px 5px;}
.tableBox table.list th{background:#fff;height:50px;line-height:50px;border-bottom:solid 1px #9ea6af;color:#0b2c55;padding:0;}
.tableBox table.list td.left{text-align:left;}
.tableBox table.list td.left span {display:block;margin-top:5px;font-size:13px;}
.tableBox table.list td.left em{background:#6e788b;color:#fff;border-radius:20px;padding:0px 10px 2px 10px;}
.tableBox table.list td.left em.on{background:#e42e1b;}
.tableBox table.list.hover tbody tr{cursor:pointer;}
.tableBox table.list.hover tbody tr:hover{background:#f7f7f7;}
.tableBox table.list.info th{background-color:#fff;}
.tableBox table.list.info th,.tableBox table.list.info td{border-left:solid 1px #d9dce2;}
.tableBox table.list.info th:first-child,.tableBox table.list.info td:first-child{border-left:none;}
.tableBox table.list.info .approva {background-color:#00beca;color:#fff;font-weight:bold;height:24px;border-radius:20px;padding:2px 10px;}

.tableBox table.form th,
.tableBox table.form td{padding:10px;}
.tableBox table.form th{ text-align:left; padding-left:40px;}
.tableBox table.form th em{color:#f74c4c;display:inline-block;vertical-align:middle;margin-left:3px;}
.tableBox table.form td *{max-width:100%;}
.tableBox table.form td input[type='text'],
.tableBox table.form td input[type='password'],
.tableBox table.form td select {min-width:178px;}
.tableBox table.form td.textContent{height:200px;line-height:160%;vertical-align:top;}
.tableBox table.form td.onlyCheck{line-height:200%;}
.tableBox table.form td.onlyText{line-height:160%;}
.tableBox table.form td div.float_r{float:right;}
.tableBox table.view th,
.tableBox table.view td{padding:5px 10px;line-height:160%;}
.tableBox table.view td *{max-width:100%;}

.paging{margin-bottom:20px;min-height:27px;/*font-size:0;*/text-align:center;}
.paging > ul > li {display:inline-block;margin:0 3px;}
.paging > ul > li > a{display:inline-block;margin:0 2px;min-width:34px;height:34px;line-height:34px;font-size:15px;text-align:center;color:#353535;border:1px solid #ccc;box-sizing:border-box;}
.paging > ul > li.current > a{background-color:#122253;color:#fff;}
.paging > ul > li.first > a{border:solid 1px #d9dce2;background:url(/images/injeinc/btn_pageFirst.png) no-repeat center center;text-indent:-9999px;}
.paging > ul > li.prev > a{border:solid 1px #d9dce2;background:url(/images/injeinc/btn_pagePrev.png) no-repeat center center;text-indent:-9999px;}
.paging > ul > li.next > a{border:solid 1px #d9dce2;background:url(/images/injeinc/btn_pageNext.png) no-repeat center center;text-indent:-9999px;}
.paging > ul > li.last > a{border:solid 1px #d9dce2;background:url(/images/injeinc/btn_pageLast.png) no-repeat center center;text-indent:-9999px;}

.btnArea{margin:20px 0;text-align:center;}
.btnArea.right{text-align:right;}
.btnArea.right.mb0{margin-bottom:0;}
.btnArea.left{text-align:left;}
.btn_ss,
.btn_s,
.btn_m,
.btn_l,
.btn_inline{display:inline-block;font-weight:bold;color:#fff;background-color:#0b2c55;vertical-align:middle;box-sizing:border-box;border-radius:2px;}
.btn_ss.on,
.btn_s.on,
.btn_m.on,
.btn_l.on,
.btn_inline.on{color:#fff;background-color:#1d398d;}
.btn_ss{margin:0 5px;padding:0 6px;height:18px;font-size:11px;line-height:16px;}
.btn_s{margin:0 5px;padding:0 8px;height:24px;font-size:12px;line-height:22px;}
.btn_m{margin:0 5px;padding:0 14px;height:30px;font-size:12px;line-height:30px;}
.btn_l{margin:0 5px;min-width:190px;padding:0 18px;height:50px;font-size:16px;line-height:50px;}
.btn_inline{padding:0 10px;height:30px;font-size:12px;line-height:28px;border-radius:5px;}
.btnArea form.inline{display:inline-block;}
.btnArea a.center{text-align:center;}


