/* 로그인 */
.login_box{padding:70px 0;background:#f5f7fa;}
.login_box p.title{margin-bottom:50px;font-size:28px;text-align:center;letter-spacing:-1.5px;color:#0b2c55;}
.login_box p.title span{font-weight:bold;}
.login_box .login_info{margin:auto;width:370px;}
.login_box .login_info span{display:block;margin-bottom:10px;padding-left:60px;height:58px;line-height:58px;background:#fff;border:1px solid #c5cad4;border-radius:3px;box-sizing:border-box;}
.login_box .login_info span.login_id{background:#fff url("/images/injeinc/icon_login_id.png") no-repeat 17px center;}
.login_box .login_info span.login_pw{background:#fff url("/images/injeinc/icon_login_pw.png") no-repeat 17px center;}
.login_box .login_info span.active{border:2px solid #000;}
.login_box .login_info span input{display:block;width:100%;border:0px none;}
.login_box .login_info span input[type="text"],
.login_box .login_info span input[type="password"]{position:relative;top:0px;height:100%;line-height:58px;}
.login_box .login_info span input:focus{outline:0px none;}
.login_box .login_info span input[type="text"]::placeholder,
.login_box .login_info span input[type="password"]::placeholder{font-size:14px;color:#c5cad4;}
.login_box .login_info span input[type="text"]::-ms-clear,
.login_box .login_info span input[type="password"]::-ms-reveal{
	display: none;
}
.login_box .login_info span input:-webkit-autofill {
   -webkit-box-shadow: 0 0 0 1000px white inset;
}
.login_box .login_info input[type="button"]{display:block;margin-bottom:40px;width:100%;height:60px;line-height:60px;font-size:17px;font-weight:bold;text-align:center;color:#fff;background:#1d398d;border:0px none;border-radius:3px;}
.login_box .login_info ul{text-align:center;}
.login_box .login_info ul::after{display:block;content:'';clear:both;}
.login_box .login_info ul li{position:relative;display:inline-block;}
.login_box .login_info ul li a{margin:0 18px;font-size:13px;color:#222;}
.login_box .login_info ul li:first-child::after{position:absolute;display:block;content:'';top:50%;right:-4px;margin-top:-2px;width:4px;height:4px;background:#c5cad4;border-radius:100%;}
