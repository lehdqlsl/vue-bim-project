.stepby {position:relative;height:95px;margin-bottom:60px;}
.stepby:after {clear:both; display:block; content:""; position:absolute;top:39%; width:100%; margin-top:-2px; border:solid 2px #d8dfe5; box-sizing:border-box;}
.stepby li {position:relative;width:20%;height:95px;float:left; text-align:center;}
.stepby li:after{clear:both; display:block; content:""; width:17px; height:17px; position:absolute;top:37%;left:50%;margin:-8px 0 0 -8px;border:solid 1px #ddd;border-radius:50%;background:#fff;z-index:2;}
.stepby li.ok:after{ position:absolute; top:0;left:50%;width:70px;height:70px; margin:0 0 0 -35px;z-index:3}
.stepby li:nth-child(1).ok:after{background:url(/images/injeinc/stepby1_bg.png) 50% 0 no-repeat;}
.stepby li:nth-child(2).ok:after{background:url(/images/injeinc/stepby2_bg.png) 50% 0 no-repeat;}
.stepby li:nth-child(3).ok:after{background:url(/images/injeinc/stepby3_bg.png) 50% 0 no-repeat;}
.stepby li:nth-child(4).ok:after{background:url(/images/injeinc/stepby4_bg.png) 50% 0 no-repeat;}
.stepby li:nth-child(5).ok:after{background:url(/images/injeinc/stepby5_bg.png) 50% 0 no-repeat;}
.stepby li span { position:absolute; bottom:20px;left:0;right:0; font-size:16px;color:#6d7882;font-weight:bold;}
.stepby li.ok span { color:#0c1531; bottom:0;}
.stepby li b { position:absolute;right:-22px;color:#6d7882;font-weight:normal;font-size:14px;}

.pproved a {display:inline-block;color:#fff;font-size:13px;background-color:#6e788b; height:32px;line-height:32px;margin-bottom:10px;padding:0 18px;border-radius:2px;}
.pproved button {display:inline-block;background-color:#fff; width:30px;height:30px;line-height:30px;margin:1px 1px 10px 1px;border-radius:2px;border:solid 1px #6e788b;text-indent:-9999px;}
