.joinComplete{padding:150px 0 60px;text-align:center;background:url("/images/injeinc/icon_joinComplete.png") no-repeat center 55px;border-top:1px solid #0f5ba9;border-bottom:1px solid #d9dce2;}
.joinComplete > p:first-child{margin-bottom:40px;font-size:29px;font-weight:bold;letter-spacing:-1.5px;color:#0b2c55;}
