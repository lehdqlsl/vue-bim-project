.modalWrap{position:fixed;top:130px;left:50%;margin:-330px 0 0 -400px;width:800px;height:660px;background:#fff;opacity:0;z-index:-1;transition-duration:0.5s;}
.modalWrap.active{top:50%;opacity:1;z-index:99;}
.modalWrap.small{margin-left:-250px;width:500px;}
.modalWrap.wide{margin-left:-450px;width:900px;}
.modalWrap .modalTitle{position:relative;padding:0 65px 0 20px;height:60px;background:#0b2c55;}
.modalWrap .modalTitle h2{display:block;font-size:20px !important;font-weight:bold;color:#fff;line-height:60px;text-align:left;}
.modalWrap .modalTitle .btn_modalClose{position:absolute;top:0;right:0;width:60px;height:60px;text-indent:-9999em;background:url(/images/injeinc/btn_modalClose.png) no-repeat center center;}
.modalWrap .modalContent{padding:20px;}
.modalWrap .modalContent .btnArea{margin:0;}
.modalWrap .modalContent .btnArea + *{margin-top:20px;}
.modalWrap .tableBox table.form th{padding:0;text-align:center;}
.modalWrap .btnArea .btn_m{min-width:150px;padding:0 18px;height:40px;font-size:16px;line-height:40px;}
